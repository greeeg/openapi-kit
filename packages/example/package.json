{
  "name": "example",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "parcel index.html",
    "build": "parcel build index.html",
    "typecheck": "tsc --noEmit",
    "openapi:pet-store": "node ../openapi-kit/dist/cli.js generate -f openapi/pet-store.yml -o ./generated/pet-store",
    "openapi:stripe": "node ../openapi-kit/dist/cli.js generate -f openapi/stripe.yml -o ./generated/stripe",
    "openapi:twilio": "node ../openapi-kit/dist/cli.js generate -f openapi/twilio.json -o ./generated/twilio",
    "openapi": "concurrently \"yarn run openapi:pet-store\" \"yarn run openapi:stripe\" \"yarn run openapi:github\""
  },
  "dependencies": {
    "@tanstack/react-query": "5.14.2",
    "query-string": "8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.18",
    "concurrently": "8.2.2",
    "openapi-kit": "*",
    "parcel": "2.10.3",
    "tsx": "4.7.0",
    "typescript": "5.3.3"
  }
}
